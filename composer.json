{
    "name": "litecms/gallery",
    "description": "Gallery package for litecms.",
    "keywords": ["lavalite", "Gallery"],
    "license": "MIT",
    "type": "lavalite-package",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Litecms\\Gallery\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litecms\\Gallery\\Providers\\GalleryServiceProvider"
            ],
            "aliases": {
                "Gallery"      : "Litecms\\Gallery\\Facades\\Gallery"
            }
        }
    },
    "minimum-stability": "dev"
}
